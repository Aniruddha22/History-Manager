{
  "name": "Play with History",
  "version": "1.2",
  "description": "Display history better",
  "permissions": [
    "history",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Play with History",
    "default_icon": "history.png"
  },
  "manifest_version": 2
}
